<!-- Tells Jasper what data and layout to use -->

<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
    http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
	name = "product_report" 
	pageHeight = "555" 
	pageWidth = "595" 
	columnWidth = "555" 
	leftMargin = "20" 
	rightMargin = "20" 
	topMargin = "20" 
	bottomMargin = "20"
	whenNoDataType="AllSectionsNoDetail">
    
    <!-- title band content displayed at the top of the first page -->
    <title>
        <band height="100">
        
        	<!-- staticText shows fixed text (report header)-->
            <staticText>
            
            	<!-- reportElement positions the element -->
                <reportElement x="0" y="10" width="555" height="40"/>
                <textElement textAlignment="Center"><font size="20" isBold="true"/></textElement>
                <text><![CDATA[Product Master Report]]></text>
            </staticText>
        </band>
    </title>
   
        
    
   
</jasperReport>